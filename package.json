{
  "name": "gt7-telemetry",
  "version": "0.3.0",
  "description": "Plugin for fetching telemetry data from Gran Turismo 7.",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
