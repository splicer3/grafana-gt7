    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Matteo Di Iorio",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "8.0.3",
    "@grafana/toolkit": "8.0.3",
    "@grafana/runtime": "8.0.3",
    "@grafana/ui": "8.0.3",
    "@types/lodash": "latest",
    "tslib": "2.2.0"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
